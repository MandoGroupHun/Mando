{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "MandoWebApp": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "IsInviteRequired": true,
    "AllowTestUsers": true
  },
  "Email": {
    "FromName": "<SENDGRID NAME>",
    "FromEmail": "<SENDGRID EMAIL>",
    "SendGridKey": "<ENV VAR>",
    "IsEnabled": false
  },
  "MariaDB": {
    "Server": "localhost",
    "Port": 3306,
    "Database": "mando",
    "Uid": "root"
  }
}
